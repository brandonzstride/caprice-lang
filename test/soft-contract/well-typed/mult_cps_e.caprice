let acc z m =
  fun r -> m (z + r)

let rec mult (dep x : int | x > 0) (dep y : int | y >= 0) 
  : let t = { z : int | z >= x * y } in (t -> t) -> t =
  fun k ->
    if x <= 0 || y <= 0
    then k 0
    else x + mult x (y - 1) (acc x k)

let check w =
  w

let main (n : int | n >= 0) : int =
  mult 100 n check
