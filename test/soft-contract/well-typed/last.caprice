(* y combinator *)
let Y f =
  fun y ->
    (fun x -> f (fun z -> (x x) z))
    (fun x -> f (fun z -> (x x) z))
    y

let is_nonempty ls =
  match ls with
  | [] -> false
  | _ -> true
  end

let last (type a') (l : list a' | is_nonempty l) : a' =
  Y (
    fun self x ->
      match x with
      | x :: xs -> 
        match xs with
        | [] -> x
        | _ -> self xs
        end
      end
  ) l