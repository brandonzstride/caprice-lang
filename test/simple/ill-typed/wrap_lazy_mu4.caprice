(* homemade list *)
let lst a = mu t.
  | `Empty of unit
  | `Cons of a * t

let f (ls : lst (int -> int)) : lst (int -> int) =
  let t : type = lst ({ i : int | i > 0 } -> int) in (* pulls on the type *)
  (* now wrap the homemade list with the once-unrolled recursive type *)
  let w : t = ls in
  w (* ERROR: the wrapper conflicts with the return type *)

