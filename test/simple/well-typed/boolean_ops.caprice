let _ : bool = false && (assert false)

let _ : bool = true || (assert false)

let f (a : bool) (b : bool) (c : bool) : { b : bool | b } =
  if a || b then
    if a then
      a
    else
      b
  else
    if a && b then
      if c then
        a
      else
        b
    else
      a || b || c || true
