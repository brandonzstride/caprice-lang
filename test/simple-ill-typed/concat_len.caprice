
let rec length x acc = 
  match x with
  | [] -> acc
  | _ :: tl -> length tl (acc + 1)
  end

let rec f (dep x : list int) (dep y : list int) (z : list int) : { a : list int | (length a 0) == (length x 0) + (length y 0) } =
  match y with
  | [] -> [] (* ERROR: needs to return x to pass the refinement *)
  | hd :: tl -> f (hd :: x) tl z
  end
    