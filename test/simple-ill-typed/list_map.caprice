
let Y = fun f ->
  (fun s -> fun x -> f (s s) x)
  (fun s -> fun x -> f (s s) x)

let f (a : type) (b : type) : unit =
  let map : (a -> b) -> list a -> list b = Y (
    fun map -> fun f -> fun ls ->
      match ls with
      | [] -> []
      | hd :: tl -> f hd :: tl (* ERROR: forgot to map tail *)
      end
  )
  in
  ()
